import java.util.Scanner;

public class Manav {
    public static void main(String[] args){
        double armut= 2.14, elma= 3.67, domates= 1.11, muz= 0.95, patlıcan=5.0, total;
        int armutKg, elmaKg, domatesKg, muzKg, patlıcanKg;
        Scanner input= new Scanner(System.in);
        System.out.print("Armut kaç kilo? : " );
        armutKg= input.nextInt();
        System.out.print("Elma kaç kilo? : " );
        elmaKg= input.nextInt();
        System.out.print("Domates kaç kilo? : " );
        domatesKg= input.nextInt();
        System.out.print("Muz kaç kilo? : " );
        muzKg= input.nextInt();
        System.out.print("Patlıcan kaç kilo? : " );
        patlıcanKg= input.nextInt();

        total= (armut*armutKg) + (elma*elmaKg) + (domates*domatesKg) + (muz*muzKg) + (patlıcan*patlıcanKg);
        System.out.print("Toplam tutar : " +total );


    }
}
